{
    "name": "yeda/subtitling",
    "description": "This library provides a set of instruments for subtitles management and supports a wide range of subtitles formats.",
    "type": "library",
    "keywords": [ "subtitles", "srt", "subrip" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Raman Yeda",
            "email": "grayfox010389@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "ext-mbstring": "*",
        "hoa/ustring": "~4.0"
    },
    "require-dev": {
        "ext-tokenizer": "*",
        "phing/phing": "2.*",
        "phpdocumentor/phpdocumentor": "^2.9",
        "sebastian/phpcpd": "^2.0",
        "phpmd/phpmd": "^2.4",
        "pdepend/pdepend": "^2.2",
        "phploc/phploc": "^3.0",
        "phpunit/phpunit": "^5.5",
        "phpunit/php-code-coverage": "^4.0",
        "squizlabs/php_codesniffer": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "Yeda\\Subtitling\\": "src/"
        },
        "exclude-from-classmap": [ "/tests/" ]
    },
    "autoload-dev": {
        "psr-4": { "Yeda\\Subtitling\\Tests\\": "tests/" }
    }
}
